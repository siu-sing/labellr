<div class="container">
    <div class="row">
        <div class="col">
            <p>Workflow - <%= job.jobName %></p>
            <p>Data Points Left - <%= textsLeft %></p>
            
        </div>
    </div>
    <div class="row">
        <div class="col">
            <% texts.forEach((text) => { %>
                <div>
                    <%- include("../text/textcard", {text:text, showOptions:true}) %>
                </div>
                <% }) %>
        </div>
    </div>
</div>